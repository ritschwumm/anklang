{
  "name": "Anklang",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "run-p sound-engine monitor",
    "sound-engine": "lib/AnklangSynthEngine $ASEOPT && false",
    "eslint": "eslint --no-eslintrc -c ui/.eslintrc.cjs -f unix --cache $(cat ui/.eslint.files)",
    "ui/rebuild": "make ui/rebuild -C $npm_package_config_srcdir --no-print-directory -j",
    "monitor": "nodemon --ext '*' --watch $npm_package_config_srcdir/ui/ --delay 99ms --on-change-only --exec 'npm run -s ui/rebuild || false' --exitcrash"
  },
  "dependencies": {
    "vue": "=3.2.45"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@fontsource/charis-sil": "^4.5.7",
    "@fontsource/inconsolata": "^4.5.8",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "browserify": "^17.0.0",
    "css-color-converter": "^2.0.0",
    "csstree-validator": "^3.0.0",
    "electron": "^21.1.0",
    "eslint": "^8.25.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.6.0",
    "icon-gen": "^3.0.1",
    "jsdoc": "^3.6.11",
    "jsdoc-api": "^8.0.0",
    "lit": "^2.5.0",
    "markdown-it": "^13.0.1",
    "mathjax": "^3.2.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-cli": "^10.0.0",
    "postcss-color-hwb": "^3.0.0",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-discard-comments": "^5.1.2",
    "postcss-discard-duplicates": "^5.1.0",
    "postcss-functions": "^4.0.2",
    "postcss-lab-function": "^4.2.1",
    "postcss-nested": "^6.0.0",
    "postcss-scss": "^4.0.5",
    "@puppeteer/replay": "^1.3.1",
    "puppeteer-core": "^18.2.1",
    "puppeteer-in-electron": "^3.0.5",
    "rollup": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass": "^1.55.0",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
    "terser": "^5.15.1",
    "zcam-js": "^0.0.9"
  }
}
