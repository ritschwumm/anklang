{
  "name": "Anklang",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "run-p sound-engine monitor",
    "sound-engine": "lib/AnklangSynthEngine-$npm_package_config_version $ASEOPT && false",
    "eslint": "eslint --no-eslintrc -c ui/.eslintrc.cjs -f unix --cache $(cat ui/.eslint.files)",
    "ui/rebuild": "make ui/rebuild -C $npm_package_config_srcdir --no-print-directory -j",
    "monitor": "nodemon --ext '*' --watch $npm_package_config_srcdir/ui/ --delay 99ms --on-change-only --exec 'npm run -s ui/rebuild || false' --exitcrash"
  },
  "dependencies": {
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@fontsource/charis-sil": "^4.3.0",
    "@fontsource/inconsolata": "^4.3.0",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "browserify": "^17.0.0",
    "chroma-js": "^2.3.0",
    "css-color-converter": "^2.0.0",
    "cssnano": "^5.1.7",
    "csstree-validator": "^3.0.0",
    "electron": "^15.1.0",
    "eslint": "^8.13.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "icon-gen": "^1.2.3",
    "jsdoc": "^3.6.7",
    "lit": "^2.2.2",
    "markdown-it": "^12.2.0",
    "mathjax": "^3.2.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "perfectionist-dfd": "^3.0.0",
    "postcss": "^8.4.12",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-cli": "^9.1.0",
    "postcss-color-hwb": "^3.0.0",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-discard-comments": "^5.1.1",
    "postcss-discard-duplicates": "^5.1.0",
    "postcss-functions": "^4.0.2",
    "postcss-lab-function": "^4.2.0",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^4.0.3",
    "rollup": "^2.70.2",
    "sass": "^1.50.1"
  }
}
