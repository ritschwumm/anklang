{
  "name": "Anklang",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "run-p sound-engine monitor",
    "sound-engine": "lib/AnklangSynthEngine $ASEOPT && false",
    "eslint": "eslint --no-eslintrc -c ui/.eslintrc.cjs -f unix --cache $(cat ui/.eslint.files)",
    "ui/rebuild": "make ui/rebuild -C $npm_package_config_srcdir --no-print-directory -j",
    "monitor": "nodemon --ext '*' --watch $npm_package_config_srcdir/ui/ --delay 99ms --on-change-only --exec 'npm run -s ui/rebuild || false' --exitcrash"
  },
  "dependencies": {
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@fontsource/charis-sil": "^4.5.5",
    "@fontsource/inconsolata": "^4.5.6",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "browserify": "^17.0.0",
    "css-color-converter": "^2.0.0",
    "csstree-validator": "^3.0.0",
    "electron": "^18.3.5",
    "eslint": "^8.16.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "icon-gen": "^3.0.1",
    "jsdoc": "^3.6.10",
    "lit": "^2.2.4",
    "markdown-it": "^13.0.1",
    "mathjax": "^3.2.1",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "perfectionist-dfd": "^3.0.0",
    "postcss": "^8.4.14",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-cli": "^9.1.0",
    "postcss-color-hwb": "^3.0.0",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-discard-comments": "^5.1.1",
    "postcss-discard-duplicates": "^5.1.0",
    "postcss-functions": "^4.0.2",
    "postcss-lab-function": "^4.2.0",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^4.0.4",
    "rollup": "^2.74.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass": "^1.52.1",
    "terser": "^5.13.1",
    "zcam-js": "=0.0.9"
  }
}
